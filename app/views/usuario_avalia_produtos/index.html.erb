<p id="notice"><%= notice %></p>

<h1>Usuario Avalia Produtos</h1>

<table class = "table table-striped">
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Produto</th>
      <th>Estrelas</th>
      <th>Comentario</th>
      <th>Dt avaliacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuario_avalia_produtos.each do |usuario_avalia_produto| %>
      <tr>
        <td><%= usuario_avalia_produto.usuario %></td>
        <td><%= usuario_avalia_produto.produto %></td>
        <td><%= usuario_avalia_produto.estrelas %></td>
        <td><%= usuario_avalia_produto.comentario %></td>
        <td><%= usuario_avalia_produto.dt_avaliacao %></td>
        <td><%= link_to 'Show', usuario_avalia_produto %></td>
        <td><%= link_to 'Edit', edit_usuario_avalia_produto_path(usuario_avalia_produto) %></td>
        <td><%= link_to 'Destroy', usuario_avalia_produto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario Avalia Produto', new_usuario_avalia_produto_path %>
